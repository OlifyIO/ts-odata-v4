{
  "version": "0.3.0",
  "name": "ts-odata",
  "description": "A typescript library to help generate odata queries",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrekkingForCharity/ts-odata.git"
  },
  "bugs": "https://github.com/TrekkingForCharity/ts-odata/issues",
  "homepage": "https://github.com/TrekkingForCharity/ts-odata",
  "author": "Trekking for Charity",
  "contributors": [
    "Andrew Davis"
  ],
  "keywords": [
    "jodata",
    "odata"
  ],
  "main": "dist/lib/index.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive test/**/*-spec.ts",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "postinstall": "tsc"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "10.9.2",
    "autodts": "0.0.6",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.12",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "3.0.1"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
